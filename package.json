{
  "name": "tap-dancer",
  "version": "0.1.2",
  "description": "a quick human-readable test-reporter for Tap",
  "keywords": [
    "reporter",
    "tap",
    "tape"
  ],
  "homepage": "https://github.com/spencermountain/tap-dance#readme",
  "bugs": {
    "url": "https://github.com/spencermountain/tap-dance/issues"
  },
  "license": "MIT",
  "author": "spencermountain@gmail.com",
  "main": "./src/index.js",
  "bin": {
    "tap-dancer": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencermountain/tap-dance.git"
  },
  "scripts": {
    "test": "exit 0",
    "build": "./node_modules/.bin/babel ./src/index.js -o ./builds/tap-dance.js",
    "watch": "tape ~/mountain/nlp-compromise/test/unit/**/*.test.js | ./src/index.js"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "stacktrace-parser": "0.1.4",
    "tap-out": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "shelljs": "^0.8.2",
    "tape": "4.9.1"
  }
}
